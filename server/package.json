{
  "name": "MM",
  "version": "1.0.0",
  "description": "Cache key-value pairs",
  "main": "'./src/main.ts'",
  "scripts": {
    "start": "node dist/main.js",
    "build": "npm test && tsc && npm run copy-files",
    "start:dev": "NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only src/main.ts",
    "copy-files": "copyfiles -u 1 src/views/layouts/*.* src/views/state/*.* src/views/*.* dist",
    "tslint": "tslint --project tsconfig.json",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --require ts-node/register 'src/test/**/*.test.ts' --exit",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create"
  },
  "author": "Julian Antonucci",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "express-validator": "^6.12.0",
    "mongoose": "^5.12.13",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/chai": "^4.2.21",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.2",
    "@types/sequelize": "^4.28.10",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "mocha": "^8.0.2",
    "nyc": "^15.1.0",
    "ts-mocha": "^8.0.0",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  }
}
